Content : WOComponentContent {
}

ColTitle : WOString {
  value = itemDict.short;
  valueWhenEmpty = itemDict.title;
  escapeHTML = false;
}

Columns : WORepetition {
  list = properties;
  item = itemDict;
}

ColHead : WOGenericContainer {
  elementName = "th";
  title = hover;
  style = style;
  rowspan = rowspan;
  colspan = itemDict.subParams.count;
}

SubHead : WOGenericContainer {
  elementName = "th";
  title = hover;
  style = style;
}

SubTitle : WOString {
  value = subDict.short;
  valueWhenEmpty = subDict.title;
  escapeHTML = false;
}

List : WORepetition {
  list = list;
  item = itemRow;
  index = ^index;
}

RowExists : WOConditional {
  condition = itemRow.editingContext;
}

ItemRow : WOGenericContainer {
  elementName = "tr";
  class = ^rowClass;
  id = rowID;
  style = ^rowStyle;
}

Cell : OnClickContainer {
  elementName = "td";
  class = ^cellClass;
  style = style;
  onclick = onClick;
  disabled = disabledClick;
  invokeAction = select;
  title = valueOf.item.itemDict.itemHover;
}

SubCell : OnClickContainer {
  elementName = "td";
  class = valueOf.item.subDict.class;
  style = style;
  disabled = disabledClick;
  onclick = onClick;
  invokeAction = select;
  title = valueOf.item.subDict.itemHover;
}

Display : DisplayAny {
  value = item;
  dict = itemDict;
}

NoSubs : WOConditional {
  condition = itemDict.subParams;
  negate = true;
}

SubParams : WORepetition {
  list = itemDict.subParams;
  item = subDict;
  index = index;
}

SubDisplay : DisplayAny {
  value = item;
  dict = subDict;
}
TableContainer : WOGenericContainer {
  elementName = "table";
  width = ^width;
  cellpadding = ^cellpadding;
  cellspacing = ^cellspacing;
  align = ^align; 
}

HasScript : WOConditional {
  condition = itemDict.script;
}

Script : WOString {
  value = itemDict.script;
  escapeHTML = false;
}
