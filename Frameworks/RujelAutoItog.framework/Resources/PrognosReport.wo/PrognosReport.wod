PrognTitle : WOString {
  value = prognosTitle;
  escapeHTML = false;
}

Mark : WOString {
  value = item.mark;
  valueWhenEmpty = "...";
}

IfTimeout : WOConditional {
  condition = item.timeout;
}

TimeoutTitle : WOString {
  value = timeoutUpTo;
}

Reason : WOString {
  value = item.reason;
}

Timeout : WOString {
  value = item.timeout;
}

Period : WOString {
  value = item.period;
}

PrognosesList : WORepetition {
  list = ^value;
  item = item;
  index = idx;
}
