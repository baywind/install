
Title : WOString {
  value = application.strings.RujelComplete_Complete.CourseCatalog;
  valueWhenEmpty = "CourseCatalog";
  escapeHTML = false;
}
Courses : WORepetition {
  list = allCourses;
  item = item;
}

Head : WOString {
  value = grHead;
  escapeHTML = false;
}

CourseTitle : WOString {
  value = present;
  escapeHTML = false;
}

ByClass : WOString {
  value = application.strings.RujelComplete_Complete.byClass;
  valueWhenEmpty = "By Class";
  escapeHTML = false;
}

BySubject : WOString {
  value = application.strings.RujelComplete_Complete.bySubject;
  valueWhenEmpty = "By Class";
  escapeHTML = false;
}

ByTeacher : WOString {
  value = application.strings.RujelComplete_Complete.byTeacher;
  valueWhenEmpty = "By Class";
  escapeHTML = false;
}
Types : WORepetition {
  list = types;
  item = grHead;
}

TypeLink : WOGenericContainer {
  elementName = "a";
  target = "_self";
  href = groupLink;
  omitTags = style;
}

GroupTitle : WOString {
  value = groupTitle;
  escapeHTML = false;
}

Sections : WORepetition {
 list = application.strings.sections.list;
 item = courseID;
}

HasSections : WOConditional {
  condition = showSections;
}

SectionTitle : WOString {
  value = courseID.value;
}

LI : WOGenericContainer {
  elementName = "li";
  style = style;
}
