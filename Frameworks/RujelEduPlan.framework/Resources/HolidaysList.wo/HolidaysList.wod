Repetition : WORepetition {
  list = list;
  item = item;
}

Exists : WOConditional {
  condition = exists;
}

ExistingRow : OnClickContainer {
  elementName = "tr";
  class = rowClass;
  disabled = session.readAccess._edit.Holiday;
  onclick = session.tryLoad;
  invokeAction = select;
  title = item.listName;
}

NameCell: OnClickContainer {
	elementName = "td";
	onclick = session.tryLoad;
	invokeAction = select;
	disabled = item.name;
}

Name : WOString {
  value = item.holidayType.name;
}

Begin : WOString {
  value = item.begin;
  dateformat = application.prefs.ui.dateFormat;
}

End : WOString {
  value =item.end;
  dateformat = application.prefs.ui.dateFormat;
}

Preset : WOConditional {
  condition = exists;
  negate = true;
}

BeginField : WOTextField {
	value = item.begin;
	dateformat = application.prefs.ui.dateFormat;
	size = 10;
	maxlength = 10;
	style = "text-align:center;";
	onkeypress = "return isNumberInput(event,true);";
	disabled = item.disabled;
}

EndField : WOTextField {
	value = item.end;
	dateformat = application.prefs.ui.dateFormat;
	size = 10;
	maxlength = 10;
	style = "text-align:center;";
	onkeypress = "return isNumberInput(event,true);";
	onfocus = "if(!value && defaultValue)value=defaultValue;";
	disabled = item.disabled;
}

Disabled : WOCheckBox {
	checked = item.disabled;
	style = "display:none;";
}

DeleteCell : OnClickContainer {
  elementName = "td";
  disabled = session.readAccess._delete.Holiday;
  onclick = "toggleDisabled(this);";
  style = "font-size:larger;";
}

Cross : WOImage {
  framework = "RujelBase";
  filename = "cross.png";
  height = 12;
  width = 12;
  style = crossStyle;
}

PresetRow: WOGenericContainer {
	elementName = "tr";
	class = rowClass;
}

Save: WOSubmitButton {
	action = save;
	value = application.strings.Reusables_Strings.uiElements.Save;
}
Form : WOForm {
}

BeginNewField : WOTextField {
	value = newDict.begin;
	dateformat = application.prefs.ui.dateFormat;
	size = 10;
	maxlength = 10;
	style = "text-align:center;";
	onkeypress = "return isNumberInput(event,true);";
	name = "beginH";
}

EndNewField : WOTextField {
	value = newDict.end;
	dateformat = application.prefs.ui.dateFormat;
	size = 10;
	maxlength = 10;
	style = "text-align:center;";
	onkeypress = "return isNumberInput(event,true);";
	onfocus = "if(!value)value=form.beginH.value;";
}

NewNameField : WOTextField {
  value = newDict.name;
  style = "width:96%;";
