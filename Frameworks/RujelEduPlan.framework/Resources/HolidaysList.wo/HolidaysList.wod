  escapeHTML = false;
}

NameTitle : WOString {
  value = application.strings.RujelEduPlan_EduPlan.properties.name;
  escapeHTML = false;
}

BeginTitle : WOString {
  value = application.strings.RujelEduPlan_EduPlan.EduPeriod.begin;
