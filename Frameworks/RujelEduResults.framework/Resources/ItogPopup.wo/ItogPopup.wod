CanChange: WOConditional {
	condition = readOnly;
	negate = true;
}

CanDelete: WOConditional {
	condition = addOn.access.delete;
}

CancelButton: WOGenericElement {
	value = application.strings.Reusables_Strings.uiElements.Cancel;
	type = "button";
	elementName = "input";
	onclick = "closePopup();";
}

DeleteButton: WOGenericElement {
	value = application.strings.Reusables_Strings.uiElements.Delete;
	invokeAction = delete;
	onclick = "form.mark.value='';form.mark.onchange();form.save.click();form.changeReason.focus();";
	elementName = "input";
	type = "button";
}

MarkTitle: WOString {
	value = application.strings.RujelEduResults_EduResults.properties.ItogMark.mark;
}

EduPeriodTitle: WOString {
	value = eduPeriod.title;
}

FlagCheckBox: WOCheckBox {
	checked = flagStatus;
	disabled = true;
}

FlagList: WORepetition {
	item = item;
	list = flaglist;
}

FlagName: WOString {
	value = flagName;
}

FlagsTitle: WOString {
	value = application.strings.RujelEduResults_EduResults.properties.ItogMark.flags.this;
}

ItogDefined: WOConditional {
	condition = itog;
}

ItogEditForm: WOForm {
	onsubmit = "return (!save.disabled);";
}

MarkField: WOTextField {
	maxlength = 10;
	size = 5;
	value = mark;
	style = "text-align:center;";
	disabled = readOnly;
	onkeypress = onkeypress;
	onchange = onkeypress;
	onblur = "form.changeReason.focus();";
	name = "mark";
}

PopupTitle: WOString {
	value = application.strings.RujelEduResults_EduResults.properties.ItogMark.this;
}

Save: WOSubmitButton {
	action = save;
	value = application.strings.Reusables_Strings.uiElements.Save;
	name = "save";
}

StudentFirstName: WOString {
	value = student.person.firstName;
}

StudentLastName: WOString {
	value = student.person.lastName;
}

ShowHistory : WOConditional {
  condition = ifArchive;
}

ArchiveTitle : ArchiveRow {
	class = "grey";
}

ArchiveList : ArchiveRow {
	class = "ungerade";
	object = itog;
	item = item;
	identifierDict = identifierDictionary;
}

MarkInArchive : WOString {
  value = item.@mark;
}

ArchiveContainer : WOGenericContainer {
  elementName = "td";
  title = item.@value;
}

ReasonTitle : WOString {
  value = application.strings.RujelArchiving_Archive.reason;
  valueWhenEmpty = "Reason";
  escapeHTML = false;
}

ChangeReason : WOText {
  value = changeReason;
  rows = "2";
  style = "width:96%;";
  name = "changeReason";
  onkeypress = "form.save.disabled = (value == null || value.length < 2);";
}
