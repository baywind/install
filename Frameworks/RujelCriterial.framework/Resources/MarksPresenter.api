<?xml version="1.0" encoding="UTF-8"?>
<wodefinitions>
	<wo class="MarksPresenter" wocomponentcontent="false">

		<binding name="selectAction"/>
		<binding defaults="Boolean" name="isSelected"/>
		<binding name="lesson"/>
		<binding name="student"/>
		<binding name="defaultStyle"/>
		<binding name="cellID"/>
		<binding defaults="Boolean" name="readOnly"/>
		<binding defaults="Boolean" name="denyCreation"/>
		<binding defaults="Boolean" name="full"/>

		<validation message="'lesson' is a required binding">
			<unbound name="lesson"/>
		</validation>

		<validation message="'student' is a required binding">
			<unbound name="student"/>
		</validation>
	<binding name="presenterCache"/>
    </wo>
</wodefinitions>
